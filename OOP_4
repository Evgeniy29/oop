#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

void print(vector<int>& a) {
    cout << "The length is: " << a.size() << '\n';

    for (int i = 0; i < a.size(); i++)
        cout << a[i] << ' ';

    cout << endl;
}

int main()
{
    vector<int> array{ 0, 1, 6, 3, 5, 4, 2, 7, 8  };

    array.pop_back();
    print(array);

    array.erase(array.begin());
    print(array);

    std::sort(array.begin(), array.end());
    print(array);

   

    if (array.empty())
        cout << "Vector is empty.\n";
    else
        cout << "Vector is not empty.\n";

    return 0;
}
